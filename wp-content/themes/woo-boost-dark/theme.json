{
	"version": 2,
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"settings": {
		"appearanceTools": true,
		"color": {
			"palette": [
				{
					"slug": "base",
					"color": "#000",
					"name": "Base"
				},
				{
					"slug": "contrast",
					"color": "#d1d1d1",
					"name": "Contrast"
				},
				{
					"slug": "primary",
					"color": "#fff",
					"name": "Primary"
				},
				{
					"slug": "secondary",
					"color": "#00a0d2",
					"name": "Secondary"
				},
				{
					"slug": "tertiary",
					"color": "#1a1919",
					"name": "Tertiary"
				},
				{
					"slug": "transparent",
					"color": "transparent",
					"name": "Transparent"
				},
				{
					"slug": "secondary-bg",
					"color": "#000000",
					"name": "Secondary Background"
				},
				{
					"slug": "table-line",
					"color": "#505050",
					"name": "Table Line"
				},
				{
					
					"slug": "theme",
					"color": "##000",
					"name": "Theme"

				}
			]
		}
	},
		"styles": {
			"blocks": {
			"core/code": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			},
			"core/comment-author-name": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-edit-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/comment-reply-link": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/navigation": {
				"typography": {
					"fontSize": "18px",
					"fontWeight": "400"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"color": {
								"text": "var(--wp--preset--color--theme)"
							},
							"typography": {
								"textDecoration": "underline"
							}
						},
						":focus": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						},
						":active": {
							"color": {
								"text": "var(--wp--preset--color--contrast)"
							}
						}
					}
				}
			},
			"core/post-author": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-date": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-navigation-link": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/post-terms": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)"
				}
			},
			"core/post-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/query-pagination": {
				"typography": {
					"fontWeight": "700"
				}
			},
			"core/query-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--x-large)"
				}
			},
			"core/site-title": {
				"typography": {
					"fontSize": "var(--wp--preset--font-size--medium)"
				},
				"elements": {
					"link": {
						"typography": {
							"textDecoration": "none"
						},
						":hover": {
							"typography": {
								"textDecoration": "underline"
							}
						}
					}
				}
			}
		},
		"elements": {
			"button": {
				"spacing": {
					"padding": {
						"top": "4px",
						"left": "34px",
						"bottom": "4px",
						"right": "34px"
					}
				},
				"border": {
					"color": "var(--wp--preset--color--transparent)",
					"width": "4px",
					"style": "solid",
					"radius": "4px"
				},
				"color": {
					"background": "var(--wp--preset--color--theme)",
					"text": "var(--wp--preset--color--base)"
				},
				"typography": {
					"fontSize": "var(--wp--preset--font-size--small)",
					"fontWeight": "700"
				}
			},
			"caption": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"cite": {
				"color": {
					"text": "var(--wp--preset--color--primary)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--theme)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					}
				}
			}
		}
	}
}
